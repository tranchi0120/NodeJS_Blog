<div class="container">
    khóa học của tôi
    <div>
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">#</th>
                    <th scope="col">name</th>
                    <th scope="col">Description</th>
                    <th scope="col">time-Create</th>
                </tr>
            </thead>
            <tbody>
                {{#each course}}
                <tr>
                    <th scope="row">{{@index plus 1}}</th>
                    <td>{{this.name}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.createdAt}}</td>

                </tr>
                {{/each}}
            </tbody>
        </table>
    </div>
</div>